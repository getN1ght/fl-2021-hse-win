{
  "scopeName": "source.abc",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [{ "include": "#cases" }, { "include": "#specifiers" }]
    },
    "cases": {
      "match": "\\b(states|links)\\b",
      "name": "keyword.control"
    },
    "specifiers": {
      "match": "\\b(beginning|terminal)\\b",
      "name": "specifiers"
    }
  }
}